               <lb/>Weib <persName key="ed_fhk_wcg_j5b">Ortha</persName>
               <lb/>Sohn <persName key="ed_ijh_ycg_j5b">Johann Gerhard</persName> 2 Jahr
               <lb/>Tochter <persName key="ed_z1h_zcg_j5b">Anna</persName> 7 <lb/>Eine Kuh, 1
